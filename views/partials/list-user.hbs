<div class="container">
  <div class="table-container">
    <table class="table is-fullwidth">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Password</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            {{users.firstName}}
          </td>
          <td>
            {{users.lastName}}
          </td>
          <td>
            {{users.email}}
          </td>
          <td>
            {{users.password}}
          </td>
          <td>
            <a href="/station/{{../station._id}}/editprofile/{{_id}}" class="button">
              {{>icons/edit}}
            </a>
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>